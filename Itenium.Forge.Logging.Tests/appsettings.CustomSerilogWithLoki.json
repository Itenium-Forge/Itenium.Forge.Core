{
  "$schema": "https://itenium-forge.github.io/appsetting-schemas/AppSettingsSchemaV1.json",
  "Forge": {
    "ServiceName": "LoggingTests-Tests",
    "TeamName": "TeamName",
    "Tenant": "TenantName",
    "Environment": "CustomSerilogWithLoki",
    "Application": "LoggingTests"
  },
  "ForgeConfiguration": {
    "Logging": {
      "FilePath": "",
      "LokiUrl": "http://loki:3100"
    }
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  }
}
